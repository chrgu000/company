@charset "utf-8";
/**
 * backboard card 60(width = 60px) style.
 */
/*
override defaults
*/
.rl_backboard .rl_card_60{
	width:60px;
}
/* ports_ctn */
.rl_card_60 .ports_ctn{
	background-image:url(../image/default/backboard/card_60_sprite_bg.gif);
	text-align:center;
}
/* temp style*/
.rl_card_60 .ports_ctn div{
	width:100%;
	height:450px;
	background:no-repeat center 26px;
}
.rl_card_60 .card_head, .rl_card_60 .card_foot{
	background-image:url(../image/default/backboard/card_60_sprite.gif);
}
.card_normal .ports_ctn{
	background-position:-60px 0;
}
.card_alarm .ports_ctn{
	background-position:-120px 0;
}
.card_mouseover .card_normal .ports_ctn{
	background-position:-180px 0;
}
.card_mouseover .card_alarm .ports_ctn{
	background-position:-240px 0;
}
.rl_card_60 .rl_port{
	left:18px;
}
