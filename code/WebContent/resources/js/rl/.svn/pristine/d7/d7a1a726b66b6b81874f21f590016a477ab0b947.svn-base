@charset "UTF-8";
.rl_tree {
    -moz-user-select:none ;/*for ff*/
	-webkit-user-select: none; /*for webkit*/
	border:solid 1px #A6C2E6;
	background:#fff;
}
.rl_tree .root_wrap{
	overflow:auto;
	_width:100%;/*for ie6*/
	height:100%;
	background:#fff;
	cursor:default;
}
.rl_tree .root{
	width:100%;/*for ie6&7*/
	float:left;
	min-width:100%;
}
.rl_tree a {
	padding-left:5px;
}
.rl_tree a:link, .rl_tree a:hover, .rl_tree a:active, .rl_tree a:visited {
	color:#000;
	text-decoration:none;
}
.rl_tree a:hover {
	text-decoration:underline;
}
.rl_tree input {
	width:16px;
	height:16px;
	vertical-align:middle;
	margin-left:3px;
}
.rl_tree .node {
	list-style:none;
	white-space:nowrap;
}
.rl_tree .elbow_indent, .rl_tree .elbow_icon, .rl_tree .node_icon {
	width:16px;
	height:18px;
	vertical-align:middle;
	background-repeat:no-repeat;
	background-position:center;
}
.rl_tree .content {
	height:18px;
	line-height:18px;
	white-space:nowrap;
	vertical-align:middle;
}
.rl_tree .focused{
	background-color:#C1DBFC;
}
.rl_tree .chd_ctn {
}
.rl_tree .collapsed{
	display:none;
}
/* tree with elbow lines */
.rl_tree_lines .elbow {
	background-image:url(../image/default/tree/elbow.gif);
}
.rl_tree_lines .elbow_s {
	background-image:url(../image/default/tree/elbow.gif);
}
.rl_tree_lines .elbow_e {
	background-image:url(../image/default/tree/elbow_end.gif);
}
.rl_tree_lines .elbow_collapsed_s {
	background-image:url(../image/default/tree/elbow_plus_nl.gif);
}
.rl_tree_lines .elbow_collapsed {
	background-image:url(../image/default/tree/elbow_plus.gif);
}
.rl_tree_lines .elbow_collapsed_e {
	background-image:url(../image/default/tree/elbow_plus_end.gif);
}
.rl_tree_lines .elbow_expanded_s {
	background-image:url(../image/default/tree/elbow_minus_nl.gif);
}
.rl_tree_lines .elbow_expanded {
	background-image:url(../image/default/tree/elbow_minus.gif);
}
.rl_tree_lines .elbow_expanded_e {
	background-image:url(../image/default/tree/elbow_minus_end.gif);
}
.rl_tree_lines .elbow_line {
	background-image:url(../image/default/tree/elbow_line.gif);
}
/* tree without elbow lines */
.rl_tree_no_lines .elbow, .rl_tree_no_lines .elbow_e, .rl_tree_no_lines .elbow_line {
	background:transparent;
}
.rl_tree_no_lines .elbow_collapsed_s, .rl_tree_no_lines .elbow_collapsed, .rl_tree_no_lines .elbow_collapsed_e {
	background-image:url(../image/default/tree/arrows.gif);
	background-position:0 4px;
	background-repeat:no-repeat;
}
.rl_tree_no_lines .elbow_expanded_s, .rl_tree_no_lines .elbow_expanded, .rl_tree_no_lines .elbow_expanded_e {
	background-image:url(../image/default/tree/arrows.gif);
	background-position:-16px 4px;
	background-repeat:no-repeat;
}
