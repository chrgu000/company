@charset "utf-8";
.rl_tab{
	float:left;
    margin:0px;
	margin-left:-1px;
}
/*override .rl_btn .with_icon */
.rl_tab .with_icon{
	padding-left:0;
}
.rl_tab .with_icon .txt_ctn{
	margin-left:20px;
}
.rl_tab .close_icon_ctn{
	width:14px;
}
.rl_tab .close_icon{
	width:14px;
	height:12px;
}
.rl_tab button{
	text-align:left;
}
.rl_tab .rl_btn_center{
	background:url(../image/default/tabpanel/tab_round_sprite.gif) repeat-x 0 -200px;
}
.rl_tab .rl_btn_left,
.rl_tab .rl_btn_right{
	background-image:url(../image/default/tabpanel/tab_round_sprite.gif);
}
.rl_tab .rl_btn_left{
    background-position:0 -150px;
}
.rl_tab .rl_btn_right{
    background-position:0 -250px;
}
.rl_tab_no_border .rl_btn_left,
.rl_tab_no_border .rl_btn_right,
.rl_tab_no_border .rl_btn_center{
	background-position:0 100px;
}
/*onmouseover*/
.rl_tab_over .rl_btn_left{
    background-position:0 0;
}
.rl_tab_over .rl_btn_center{
    background-position:0 -50px;
}
.rl_tab_over .rl_btn_right{
    background-position:0 -100px;
}
/*onmousedown || actived*/
.rl_tab_down .rl_btn_left{
    background-position:0 -300px;
}
.rl_tab_down .rl_btn_center{
    background-position:0 -350px;
}
.rl_tab_down .rl_btn_right{
    background-position:0 -400px;
}
/*
focused, this is same as actived state at version 1.0;
*/
.rl_tab_focused .rl_btn_left{
    background-position:0 -300px;
}
.rl_tab_focused .rl_btn_center{
    background-position:0 -350px;
}
.rl_tab_focused .rl_btn_right{
    background-position:0 -400px;
}
/*disabled*/
.rl_tab_disabled .rl_btn_left{
    background-position:0 -450px;
}
.rl_tab_disabled .rl_btn_center{
    background-position:0 -500px;
}
.rl_tab_disabled .rl_btn_right{
    background-position:0 -550px;
}
.rl_tab .close_icon{
	background:url(../image/default/tabpanel/close_sprite_1.gif) no-repeat 0 2px;	
}
.rl_tab .close_icon_over{
    background-position:0 -10px;
}
.rl_tab .close_icon_down{
    background-position:0 -22px;
}
/*
tab themes
page_tab : normal style for page tab.
page_btab : normal style for page tab which usually stay at content bottom.
*/
.rl_tab .page_tab{
}
.rl_tab .page_btab{
}
.rl_tab .page_btab .rl_btn_center{
	background:url(../image/default/tabpanel/tab_b_round_sprite.gif) repeat-x 0 -82px;
}
.rl_tab .page_btab .rl_btn_left,
.rl_tab .page_btab .rl_btn_right{
	background-image:url(../image/default/tabpanel/tab_b_round_sprite.gif);
}
.rl_tab .page_btab .rl_btn_left{
    background-position:0 -22px;
}
.rl_tab .page_btab .rl_btn_right{
    background-position:right -142px;
}
/*onmouseover*/
.rl_tab_over .page_btab .rl_btn_left{
    background-position:0 -202px;
}
.rl_tab_over .page_btab .rl_btn_center{
    background-position:0 -262px;
}
.rl_tab_over .page_btab .rl_btn_right{
    background-position:right -322px;
}
/*onmousedown || actived*/
.rl_tab_down .page_btab .rl_btn_left{
    background-position:0 -202px;
    /*background-position:0 -502px;*/
}
.rl_tab_down .page_btab .rl_btn_center{
    background-position:0 -262px;
    /*background-position:0 -442px;*/
}
.rl_tab_down .page_btab .rl_btn_right{
    background-position:right -322px;
    /*background-position:right -382px;*/
}
/*
focused, this is same as actived state at version 1.0;
*/
.rl_tab_focused .page_btab .rl_btn_left{
    background-position:0 -382px;
}
.rl_tab_focused .page_btab .rl_btn_center{
    background-position:0 -442px;
}
.rl_tab_focused .page_btab .rl_btn_right{
    background-position:right -502px;
}
/*disabled, tmp use default instead for no corresponed style prepared*/
.rl_tab_disabled .page_btab .rl_btn_left{
    background-position:0 -22px;
}
.rl_tab_disabled .page_btab .rl_btn_center{
    background-position:0 -82px;
}
.rl_tab_disabled .page_btab .rl_btn_right{
    background-position:right -142px;
}

.rl_tabbar{
	position:relative;
	left:0;
	top:0;
	overflow:hidden;
	/*width:100%;*/
}
.rl_tabbar .rl_tab{
}
.rl_tabbar .rl_tab button{
	color:#333;
}
.rl_tabbar .rl_tab_disabled button{
	color:gray;
}
.rl_tabbar .active_tab button{
	color:#000;
}
.rl_tabbar .rl_tab_ctn_wrap{
	position:relative;
	left:0;
	top:0;
	height:100%;
	width:100%;
	overflow:hidden;
}
.rl_tabbar .rl_tab_ctn{
	position:absolute;
	left:0;
	top:0;
	height:100%;
	/*width:200%;*/
	
	/*
	the style "margin-left:-1px" of rl_tab makes the left part of tab 
	to be unvisiable.To fix it, we add "padding-left:2px" style to the 
	tab's ctn(rl_tabbar) for ie6, and add "margin-left:1px" style for others;
	*/
	margin-left:1px !important;
	margin-left:0;
	padding-left:0 !important;
	padding-left:2px;
}

.rl_tabbar .scroller_b, .rl_tabbar .scroller_f{
	position:absolute;
	top:0;
	left:0;
}
.rl_tabbar .scroller_f{
	left:auto;
	right:0;
}

/*
rl_page_tabbar:normal style for page nav.
*/
.rl_page_tabbar {
	background:url(../image/default/tabpanel/page_tabbar_bg.gif) repeat-x top;
	height:31px;
}
.rl_page_tabbar .rl_tab{
	margin-top:2px;
	height:24px;
}

.rl_page_tabbar .active_tab{
	margin-top:0px;
	height:27px;
}
.rl_page_tabbar .rl_tab_ctn_wrap{
	height:31px;
}
.rl_page_tabbar .scroller_b{
}
/*
rl_page_btabbar: normal style for page nav.
*/
.rl_page_btabbar {
	background:url(../image/default/tabpanel/page_btabbar_bg.gif) repeat-x bottom;
	height:37px;
}
.rl_page_btabbar .rl_tab{
	margin-top:10px;
	height:26px;
}

.rl_page_btabbar .active_tab{
	margin-top:9px;
	height:28px;
}
.rl_page_btabbar .rl_tab_ctn_wrap{
	height:37px;
}
.rl_page_btabbar .scroller_b{
}

/*
rl_inline_tabbar: for inline content nav.
*/
.rl_inline_tabbar {
	background:url(../image/default/tabpanel/inline_tab_bar_bg.gif) repeat-x bottom;
	height:25px;
}
.rl_inline_tabbar .rl_tab{
	margin-top:2px;
	height:21px;
}
.rl_inline_tabbar .active_tab{
	margin-top:0px;
	height:23px;
}
.rl_inline_tabbar .scroller_b{
}

/*
tab body style.
*/
.rl_tabbody_stack{
	width:100%;
	height:100%;
	background-color:#fff;
}

.rl_tabbody_stack .tabbody_wrap{
	position:relative;
	left:0;
	top:0;
	width:100%;
	height:100%;
	overflow:hidden;
}
.rl_tabbody_stack .active_body{
	left:0;
	top:0;
	z-index:1;
}
.rl_tabbody_item{
	position:absolute;
	left:-10000px;
	top:-10000px;
	width:100%;
	height:100%;
	z-index:0;
	background:#fff;
}
/*
tab panel
*/
.rl_tabpanel{
	width:100%;
	height:100%;
}
.rl_tabpanel .head_bar_ctn{
	width:100%;
}
.rl_tabpanel .body_stack_ctn{
	width:100%;
	height:100%;
	border:solid 1px #9DB6D5;
	border-top:none;
}
