@charset "utf-8";
/**
 * underline_plot.css: plot underline as item value.
 */

.rl_underline_plot{
	list-style:none;
}
.rl_underline_plot li{
	padding:8px 0;
	text-decoration:none;
}
.rl_underline_plot a{
	text-decoration:none;
}
.rl_underline_plot a:hover{
	color:#F90;
}
.rl_underline_plot .point{
	float:right;
}
.rl_underline_plot .line_wrap{
	margin-top:3px;
	height:2px;
	background-color:#eee;
}
.rl_underline_plot .line{
	height:100%;
	background-color:#0085CC;
}
