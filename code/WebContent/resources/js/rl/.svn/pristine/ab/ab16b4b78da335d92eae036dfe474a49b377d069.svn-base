@charset "utf-8";
.rl_datepicker{
	width:200px;
	height:215px;
	overflow:hidden;
	position:absolute;
	z-index:100;
	left:0;
	top:0;
	cursor:default;
	border:solid 1px #A6C2E6;
	background-color:#fff;
	font-size:12px;
}
.rl_datepicker a{
	text-decoration:none;
}
.rl_datepicker .head{
	width:100%;
	border:none;
	height:25px;
	line-height:25px;
	overflow:hidden;
	background-color:#D1E7F5;
	border-top:solid 1px #EAF3FA;
}
.rl_datepicker .head .navbar{
	/*width:60px;*/
}
/*
override icon button style
*/
.rl_datepicker .head .navbar .rl_icon_btn{
	float:left;
}

.rl_datepicker .head .ymctrl_ctn{
	width:120px;
	text-align:right;
}
.rl_datepicker .head .ymctrl_ctn img{
	width:7px;
	height:5px;
	border:none;
}
.rl_datepicker .head .ymctrl_ctn a{
	color:#000;
	font-weight:bold;
	display:block;
}
.rl_datepicker .head .ymctrl_ctn a:hover{
	color:#0066CC;
}
.rl_datepicker .body{
	width:100%;
	height:189px !important;
	height:190px;
	position:relative;
	left:0;
	top:0;
	overflow:hidden;
}
.rl_datepicker .day_pane,
.rl_datepicker .ym_pane,
.rl_datepicker .bottombar{
	position:absolute;
	left:0;
	z-index:0;
	width:100%;
	border:none;
	text-align:right;
	background-color:#fff;
}
.rl_datepicker .day_pane{
	top:0;
	height:167px !important;
	height:168px;
}
.rl_datepicker .day_pane th{
	font-weight:lighter;
	text-align:center;
	border-top:solid 1px #fff;
	border-bottom:solid 1px #eee;
	background-color:#F0F8FB;
	padding:2px 0;
	height:16px;
	line-height:16px;
}
.rl_datepicker .day_pane td{
	height:16px;
	line-height:16px;
}
.rl_datepicker .day_pane a,
.rl_datepicker .ym_pane a{
	color:#000;
	display:block;
	height:100%;
}
.rl_datepicker .day_pane a{
	padding:4px 7px;
}
.rl_datepicker .day_pane a:hover,
.rl_datepicker .ym_pane a:hover{
	color:#0066CC;
	border:solid 1px #BEE1FF;
	background:#EDF5FE url(../image/default/calendar/cell_over.gif) repeat-x 0 0;
}
.rl_datepicker .day_pane a:hover{
	padding:3px 6px;
}
.rl_datepicker .day_pane .cell_extra a,
.rl_datepicker .ym_pane .cell_extra a{
	color:#999;
}
.rl_datepicker .day_pane .cell_extra a:hover,
.rl_datepicker .ym_pane .cell_extra a:hover{
	color:#ccc;
}
.rl_datepicker .day_pane .cell_disabled a,
.rl_datepicker .ym_pane .cell_disabled a{
	/*color:#999;
	background-color:#eee;*/
}
.rl_datepicker .day_pane .cell_selected a,
.rl_datepicker .ym_pane .cell_selected a{
	background-color:#DFEDFE;
	border:solid 1px #BEE1FF;
	background-image:url(../image/default/calendar/cell_selected.gif);
}
.rl_datepicker .day_pane .cell_selected a:hover,
.rl_datepicker .ym_pane .cell_selected a:hover{
	background-color:#EDF5FE;
	background-image:url(../image/default/calendar/cell_over.gif);
}
.rl_datepicker .day_pane .cell_selected a{
	padding:3px 6px;
}
.rl_datepicker .day_pane .cell_today a{
	padding:3px 6px;
	border:solid 1px #C00;
}
.rl_datepicker .ym_pane{
	z-index:1;
	top:-189px;
	height:100%;
	text-align:center;
}
.rl_datepicker .ym_pane td{
	width:40px;
	height:26px;
}
.rl_datepicker .ym_pane a{
	height:26px;
	line-height:26px;
	padding:1px;
}
.rl_datepicker .ym_pane a:hover{
	padding:0;
}
.rl_datepicker .ym_pane .cell_selected a{
	padding:0;
}
.rl_datepicker .ym_pane .cell_selected a:hover{
	padding:0;
}
.rl_datepicker .day_pane .cell_today a{
	padding:3px 6px;
	border:solid 1px #C00;
}
.rl_datepicker .ym_pane .y_pane,
.rl_datepicker .ym_pane .m_pane{
	width:95%;
	margin:0 auto;
}
.rl_datepicker .ym_pane .y_pane{
	margin-top:5px;
}
.rl_datepicker .ym_pane .sep_line{
	width:95%;
	height:5px;
	margin:5px auto 0 auto;
	border-top:solid 1px #eee;
	font-size:1px;
	overflow:hidden;
}
.rl_datepicker .bottombar{
	width:100%;
	top:167px !important;
	top:168px;
	height:22px;
	border:none;
	background-color:#D1E7F5;
}
.rl_datepicker .bottombar .timeselect_ctn{
	padding-left:6px;
	text-align:left;
}
.rl_datepicker .bottombar .timeselect_ctn .rl_timeselect .wrapper{
	border-color:#CEE0F5;
}
.rl_datepicker .bottombar .okctrl{
	width:31px;
}
.rl_datepicker .bottombar .okctrl a{
	display:block;
	width:31px;
	height:22px;
	background:url(../image/default/calendar/ok_sprite.gif) no-repeat 0 0;
}
.rl_datepicker .bottombar .okctrl a:hover{
	background-position:-32px 0;
}
