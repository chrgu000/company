@charset "utf-8";
html, body{
	width:100%;
	height:100%;
	margin:0;
	padding:0;
}
.rl_mui_muqtle_full_view {
	width:100%;
	height:100%;
	overflow:hidden;
}
.rl_mui_muqtle {
	position:absolute;
	left:0;
	top:0;
	width:100%;
	height:100%;
	background:url(../image/default/mui/muqtle/head_bg.gif) repeat-x;
	cursor:default;
}
.rl_mui_muqtle .logo_ctrl_ctn {
	position:absolute;
	z-index:1;
	left:3px;
	top:7px;
	width:52px;
	height:52px;
	background-image:url(../image/default/mui/muqtle/logo_sprite.gif);
}
.rl_mui_muqtle .logo_ctrl_ctn_over{
	background-position:0 -52px;
}

/**
 * comps styles
 */
.rl_mui_muqtle .head_bar {
	height:37px;
}
.rl_mui_muqtle .head_bar .quick_access_area {
	background:url(../image/default/mui/muqtle/quickaccess_bg.gif) repeat-x;
	height:28px;
}
.rl_mui_muqtle .head_bar .quick_access_bar_conner {
	background:url(../image/default/mui/muqtle/quickaccess_conner.gif) no-repeat right;
	width:18px;
}
.rl_mui_muqtle .menu_ctn,
.rl_mui_muqtle .user_ctrl_ctn{
	float:right;
}
.rl_mui_muqtle .user_ctrl_ctn{
	margin-left:30px;
}
.rl_mui_muqtle .quick_access_bar_ctn {
	padding-left:56px;
}
.rl_mui_muqtle .eixt_ctrl_ctn{
	float:right;
	width:32px;
	height:29px;
	background:url(../image/default/mui/muqtle/exit_sprite.jpg) no-repeat 0 0;
}
.rl_mui_muqtle .eixt_ctrl_ctn_over{
	background-position:0 -29px;
}
/* override defaults */
.rl_mui_muqtle .rl_page_tabbar{
	background:none;
}